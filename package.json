{
  "name": "dato-nextjs-utils",
  "version": "1.0.188",
  "description": "Utils for Bebe",
  "types": "./index.d.ts",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.modern.mjs",
  "exports": {
    "./components": {
      "require": "./components/index.cjs",
      "import": "./components/index.modern.mjs"
    },
    "./hoc": {
      "require": "./hoc/index.cjs",
      "import": "./hoc/index.modern.mjs"
    },
    "./hooks": {
      "require": "./hooks/index.cjs",
      "import": "./hooks/index.modern.mjs"
    },
    "./api": {
      "require": "./api/index.cjs",
      "import": "./api/index.modern.mjs"
    },
    ".": {
      "require": "./index.cjs",
      "import": "./index.modern.mjs"
    }
  },
  "engines": {
    "node": ">=0.16"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm version patch --force && npm run build && git add . && git commit -m \"updatez\" && git push",
    "build": "bash build.sh",
    "dev": "microbundle watch"
  },
  "author": "Bebe Jane",
  "license": "ISC",
  "dependencies": {
    "markdown-truncate": "^1.0.4",
    "remark-breaks": "^3.0.2",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^3.0.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@types/node": "16 - 18",
    "@apollo/client": "3 - 4",
    "@types/react": "17 - 18",
    "@types/react-dom": "17 - 18",
    "@datocms/cma-client": ">1.1.0",
    "next": "12 - 13",
    "react": "16 - 18",
    "react-dom": "16 - 18",
    "next-seo": ">5.5.0"
  },
  "devDependencies": {
    "microbundle": "^0.15.1"
  }
